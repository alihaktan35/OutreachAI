# OutreachAI Environment Configuration
# Copy this file to .env and fill in your actual values

# n8n Webhook URLs
# Get these from your n8n instance after setting up the workflows
N8N_WEBHOOK_LAUNCH_CAMPAIGN=https://your-n8n-instance.com/webhook/launch-campaign
N8N_WEBHOOK_CHECK_STATUS=https://your-n8n-instance.com/webhook/campaign-status
N8N_WEBHOOK_PREVIEW_EMAIL=https://your-n8n-instance.com/webhook/preview-email

# API Keys (if needed for direct integrations)
APOLLO_API_KEY=your_apollo_api_key_here
SENDGRID_API_KEY=your_sendgrid_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Application Settings
APP_ENV=development
APP_DEBUG=true

# Optional: CRM Integration
HUBSPOT_API_KEY=your_hubspot_api_key_here
PIPEDRIVE_API_KEY=your_pipedrive_api_key_here
